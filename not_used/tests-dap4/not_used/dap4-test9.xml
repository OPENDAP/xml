<?xml version="1.0"?>
<Dataset name="test1" dapVersion="4.0" xml:base="file:/Users/jimg/src/shrew/xml/dap/tests-dap4/dap-4-test1.xml"
    xmlns="http://xml.opendap.org/ns/DAP/4.0#" 
    xmlns:dap="http://xml.opendap.org/ns/DAP/4.0#"
    xmlns:xml="http://www.w3.org/XML/1998/namespace" 
    xmlns:xlink="http://www.w3.org/1999/xlink"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xml.opendap.org/ns/DAP/4.0#  file:/Users/jimg/src/shrew/xml/dap/dap4.xsd">
        
    <Group>
        <Enumeration name="mask_values">
            <elt name="clear" value="1"/>
            <elt name="cloud" value="2"/>
        </Enumeration>
        
        <Grid name="SST">
            <SharedDimension name="latitude" size="1024"/>
            <SharedDimension name="longitude" size="1024"/>
            
            <Map name="latitude" type="Float32">
                <Attribute name="maybe" type="UByte"><value>255</value></Attribute>
                <dimension ref="latitude"/>
            </Map>
            
            <Map name="longitude" type="Float32">
                <dimension ref="longitude"/>
            </Map>
            
            <Byte name="SST">
                <Attribute name="long_name" type="String">
                    <value>Sea Surface Temperature</value>
                </Attribute>
                <dimension ref="latitude"/>
                <dimension ref="longitude"/>
                
                <map ref="latitude"/>
                <map ref="longitude"/>
            </Byte>
            
            <Enum type="mask_values" name="mask">
                <Attribute name="long_name" type="String">
                    <value>Cloud mask</value>
                </Attribute>
                <dimension ref="latitude"/>
                <dimension ref="longitude"/>
                
                <map ref="latitude"/>
                <map ref="longitude"/>
            </Enum>
        </Grid>
        
    </Group>
</Dataset>